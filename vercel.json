{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/maps",
      "destination": "http://cube.node.plutonodes.com:25000/"
    },
    {
      "source": "/maps/",
      "destination": "http://cube.node.plutonodes.com:25000/"
    },
    {
      "source": "/maps/:path*",
      "destination": "http://cube.node.plutonodes.com:25000/:path*"
    }
  ]
}